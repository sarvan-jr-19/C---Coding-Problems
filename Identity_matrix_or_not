#include <stdio.h>
int main() {
    int row,col,i,j,isIdentity=1;
    scanf("%d%d", &row,&col);
    int arr[row][col];
     
    for(int i=0;i<row;i++){
        for(int j=0;j<col;j++){
            scanf("%d", &arr[i][j]);
        }
    }

    for(i=0;i<row;i++){
        for(j=0;j<col;j++){
            if (i==j && arr[i][j]!=1){
                isIdentity=0;
            } 
            else if(i!=j && arr[i][j]!=0){
                isIdentity = 0;
            }
        }
    }

    if (isIdentity)
        printf("Identity Matrix.");
    else
        printf("NOT an Identity Matrix.");

    return 0;
}

Input-1:
3 3
1 0 0
0 1 0
1 0 0

Output-1:
Identity Matrix

Input-1:
3 3
1 2 3
0 3 1
1 0 0

Output-1:
Not an Identity Matrix





